{
  "name": "@repo/resume",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "bunchee && pnpm copy-resume",
    "dev": "bunchee --watch",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "copy-resume": "cp src/data/daniel-resume-2025.md dist/es/daniel-resume-2025.md && cp src/data/daniel-resume-2025.md dist/cjs/daniel-resume-2025.md"
  },
  "jest": {
    "preset": "@repo/jest-presets/node"
  },
  "dependencies": {
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@jest/globals": "30.0.0",
    "@repo/jest-presets": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^24.0.3",
    "bunchee": "^6.5.3",
    "cpy-cli": "^5.0.0",
    "jest": "^30.0.0",
    "typescript": "5.8.3"
  }
}